<mvc:View controllerName="Gists.Test2.controller.View2" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<List items = "{main>/}" id="myList">
							<items>
								<StandardListItem
									title="{main>text}"
									description="кликов по ссылке : {main>click}"
									type="Navigation"
									press="on1stLvlPress"/>
							</items>
						</List>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
