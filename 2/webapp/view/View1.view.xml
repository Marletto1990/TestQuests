<mvc:View
	controllerName="Gists.Test2.controller.View1"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content>
						<Toolbar height="3em">
						<Button icon="sap-icon://nav-back" press="navBack"/>
      			</Toolbar>
						<List items = "{main>/}" id="myList">
							<items>
								<core:Fragment fragmentName="Gists.Test2.view.fragments.itemTemplate" type="XML"/>
							</items>
						</List>
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
