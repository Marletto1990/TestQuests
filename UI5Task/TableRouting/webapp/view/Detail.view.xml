<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml"
	controllerName="QuickStartApplication.controller.Detail">
	<App>
		<pages>
			<Page xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" title="{i18n>detailTitle}">
				<content>
					<sap.ui.layout.form:SimpleForm xmlns:sap.ui.layout.form="sap.ui.layout.form" editable="true" layout="ResponsiveGridLayout" id="form0">
						<sap.ui.layout.form:content>
							<Label text="ShipCity" width="100%" class="sapUiLargeMarginEnd"/>
							<Text text="{ShipCity}"/>
							<Label text="ShipName" width="100%" class="sapUiLargeMarginEnd"/>
							<Text text="{ShipName}" id="text1"/>
							<Label text="ShipCountry" width="100%" class="sapUiLargeMarginEnd"/>
							<Text text="{ShipCountry}" id="text2"/>
						</sap.ui.layout.form:content>
					</sap.ui.layout.form:SimpleForm>
				</content>
				<customHeader>
					<Toolbar width="100%" id="toolbar1">
						<content>
							<Button xmlns="sap.m" id="button1" press="action" cd:wiring="\{'press':\{'navigation':\{'routeName':'View1'\}\}\}"
								icon="sap-icon://nav-back"/>
						</content>
					</Toolbar>
				</customHeader>
			</Page>
		</pages>
	</App>
</mvc:View>